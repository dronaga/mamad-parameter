{
  "manifest_version": 3,
  "name": "mamad parameter extention",
  "version": "1.1",
  "description": "Advanced parameter finder: GET, POST, hidden, cookies, JS, AJAX.",
  "permissions": ["scripting", "activeTab", "cookies", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
} 